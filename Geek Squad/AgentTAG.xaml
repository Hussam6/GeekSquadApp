<Page
    x:Class="Geek_Squad.AgentTAG"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Geek_Squad"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:Name="enterBtn" Click="OnClickEnterBtn" Content="Search" Width="500" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,360,0,0" Height="32"/>
        <TextBox x:Name="nameTb" Text="" Width="500" HorizontalAlignment="Center"  Margin="0,303,0,0" VerticalAlignment="Top" Height="32" PlaceholderText="Enter Tag Number"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,51,0,0" Text="TAG Lookup" TextWrapping="Wrap" VerticalAlignment="Top" Height="38" Width="301" FontSize="24" TextAlignment="Center"/>
    </Grid>
</Page>
